{
    "name": "fahadalihyd/payprov2",
    "description": "PayPro PHP Sdk",
    "keywords": [
        "fahadalihyd",
        "payprov2"
    ],
    "homepage": "https://github.com/fahadalihyd/payprov2",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Fahad Ali",
            "email": "fahadali1000000@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7|^8",
        "illuminate/database": "5.8.*|^6|^7|^8",
        "illuminate/filesystem": "5.8.*|^6|^7|^8",
        "illuminate/http": "5.8.*|^6|^7|^8",
        "illuminate/support": "5.8.*|^6|^7|^8",
        "illuminate/view": "5.8.*|^6|^7|^8"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Fahadalihyd\\Payprov2\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fahadalihyd\\Payprov2\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Fahadalihyd\\Payprov2\\Payprov2ServiceProvider"
            ],
            "aliases": {
                "Payprov2": "Fahadalihyd\\Payprov2\\Payprov2Facade"
            }
        }
    }
}
